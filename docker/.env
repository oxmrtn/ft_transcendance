# .env

# ===========================
# CONFIGURATION DEVELOPPEMENT
# ===========================

# Désactiver la télémétrie Next.js (Privacy)
NEXT_TELEMETRY_DISABLED=1

# Hot Reloading (Mettre à 'true' si sur Windows/WSL, 'false' sinon)
# Permet de forcer la détection des changements de fichiers
WATCHPACK_POLLING=false
CHOKIDAR_USEPOLLING=false